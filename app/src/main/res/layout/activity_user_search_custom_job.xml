<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dev.r19.eeragijn.UserSearchCustomJob">

    <TextView
        android:id="@+id/textView59"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="114dp"
        android:textColor="#000000"
        android:layout_marginTop="71dp"
        android:text="search job that you prefer" />

    <TextView
        android:id="@+id/textView60"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView59"
        android:layout_below="@+id/textView59"
        android:layout_marginStart="11dp"
        android:layout_marginTop="48dp"
        android:textColor="#000000"
        android:text="Check the category" />

    <Spinner
        android:id="@+id/category_search"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:layout_below="@+id/textView60"
        android:textColor="#000000"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/custom_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView60"
        android:layout_below="@+id/category_search"
        android:layout_marginStart="21dp"
        android:layout_marginTop="30dp"
        android:textColor="#000000"
        android:text="Search" />
</RelativeLayout>
