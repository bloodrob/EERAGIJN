<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back_2"
    tools:context="com.dev.r19.SakoriSarothiClient.UnSignUserMainActivity"
    android:orientation="horizontal">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:background="@drawable/unsignuser"
        android:id="@+id/frame21">

        <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:src="@drawable/userhomelogo"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="19dp"/>
        <TextView
            android:layout_width="170dp"
            android:layout_height="120dp"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="210dp"
            android:text="@string/un_sign_user"
            android:textStyle="bold"
            android:textSize="16dp"/>
    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/frame21"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="30dp">
            <ListView
                android:id="@+id/list_of_job"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"/>
    </FrameLayout>

</RelativeLayout>
