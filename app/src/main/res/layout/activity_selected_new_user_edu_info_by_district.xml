                android:layout_marginTop="37dp" />

            <TextView
                android:id="@+id/view_postgraduate"
                android:layout_width="150dp"
                android:layout_height="30dp"
                android:layout_marginEnd="42dp"
                android:layout_alignBaseline="@+id/textView_postgraduateU"
                android:layout_alignBottom="@+id/textView_postgraduateU"
                android:layout_alignParentEnd="true" />

            <TextView
                android:id="@+id/textView_postgraduatepass"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView_postgraduateU"
                android:layout_below="@+id/textView_postgraduateU"
                android:layout_marginTop="37dp"
                android:text="Pass Year"
                android:textColor="#000000"/>

            <TextView
                android:id="@+id/view_postgraduate_pass_year"
                android:layout_width="150dp"
                android:layout_height="30dp"
                android:layout_alignBaseline="@+id/textView_postgraduatepass"
                android:layout_alignBottom="@+id/textView_postgraduatepass"
                android:layout_alignStart="@+id/view_postgraduateU" />

            <TextView
                android:id="@+id/textView_postgraduateparcentage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="37dp"
                android:text="Percantage"
                android:textColor="#000000"
                android:layout_below="@+id/textView_postgraduatepass"
                android:layout_alignStart="@+id/textView_postgraduatepass" />

            <TextView
                android:id="@+id/view_postgraduate_percantage"
                android:layout_width="150dp"
