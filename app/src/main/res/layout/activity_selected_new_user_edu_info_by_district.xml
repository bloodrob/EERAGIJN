                android:textColor="#000000"/>

            <TextView
                android:id="@+id/view_hs_pass_year"
                android:layout_width="150dp"
                android:layout_height="30dp"
                android:layout_alignBaseline="@+id/textView_hspass"
                android:layout_alignBottom="@+id/textView_hspass"
                android:layout_alignStart="@+id/view_hslc_board" />

            <TextView
                android:id="@+id/textView_hsparcentage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="37dp"
                android:text="Percantage"
                android:textColor="#000000"
                android:layout_below="@+id/textView_hspass"
                android:layout_alignStart="@+id/textView_hspass" />

            <TextView
                android:id="@+id/view_hs_percantage"
                android:layout_width="150dp"
                android:layout_height="30dp"
                android:layout_alignBaseline="@+id/textView_hsparcentage"
                android:layout_alignBottom="@+id/textView_hsparcentage"
                android:layout_alignStart="@+id/view_hs_pass_year" />
            <TextView
                android:id="@+id/textView_graduate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="85dp"
                android:text="GRADUATE"
                android:textStyle="bold"
                android:textSize="20dp"
                android:textColor="#000000"
                android:layout_below="@+id/textView_hsparcentage"
                android:layout_alignParentStart="true" />

            <TextView
                android:id="@+id/textView_graduateU"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="GRADUATE"
                android:textColor="#000000"
                android:layout_below="@+id/textView_graduate"
                android:layout_alignStart="@+id/textView_graduate"
                android:layout_marginTop="37dp" />

            <TextView
                android:id="@+id/view_graduate"
                android:layout_width="150dp"
                android:layout_height="30dp"
                android:layout_marginEnd="42dp"
                android:layout_alignBaseline="@+id/textView_graduateU"
                android:layout_alignBottom="@+id/textView_graduateU"
                android:layout_alignParentEnd="true" />

            <TextView
                android:id="@+id/textView_graduatepass"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView_graduateU"
                android:layout_below="@+id/textView_graduateU"
                android:layout_marginTop="37dp"
                android:text="Pass Year"
                android:textColor="#000000"/>

            <TextView
                android:id="@+id/view_graduate_pass_year"
                android:layout_width="150dp"
                android:layout_height="30dp"
