<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back_2"
    tools:context="com.dev.r19.eeragijn.UserDocumentUpload">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginLeft="80dp"
        android:text="Here you have to uploads the document"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:textColor="#000000"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView10">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/Browse_self_image"
                android:layout_width="310px"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="Browse"
                android:layout_marginEnd="29dp"
                android:layout_alignParentEnd="true" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignTop="@+id/Browse_self_image"
                android:layout_marginStart="48dp"
                android:layout_marginTop="10dp"
                android:textColor="#000000"
                android:text="Your image" />

            <ImageView
                android:id="@+id/Self_imageView"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_marginEnd="23dp"
                android:layout_below="@+id/Browse_self_image"
                android:layout_alignEnd="@+id/Browse_self_image"
                android:layout_marginTop="11dp" />


            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/textView11"
                android:layout_below="@+id/Self_imageView"
                android:layout_marginTop="36dp"
                android:textColor="#000000"
                android:text="Your Signature" />

            <Button
                android:id="@+id/Browse_sign_image"
                android:layout_width="310px"
                android:layout_height="wrap_content"
                android:text="Browse Signature"
                android:layout_below="@+id/Self_imageView"
                android:layout_marginTop="10dp"
                android:layout_alignStart="@+id/Browse_self_image" />

            <ImageView
                android:id="@+id/sign_ImageView"
                android:layout_width="170px"
                android:layout_height="100px"
                android:layout_alignStart="@+id/Self_imageView"
                android:layout_below="@+id/Browse_sign_image"
                android:layout_marginTop="17dp"
                />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView11"
                android:layout_below="@+id/sign_ImageView"
                android:layout_marginTop="33dp"
                android:textColor="#000000"
                android:text="your hslc" />

            <Button
                android:id="@+id/Browse_hslc_image"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/Browse_sign_image"
                android:layout_below="@+id/sign_ImageView"
                android:layout_marginTop="21dp"
                android:text="Browse HSLC" />

            <ImageView
                android:id="@+id/hslc_ImageView"
                android:layout_width="170px"
                android:layout_height="100px"
                android:layout_alignStart="@+id/Browse_sign_image"
                android:layout_below="@+id/Browse_hslc_image"
                android:layout_marginStart="28dp"
                android:layout_marginTop="16dp"
                />
            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView11"
                android:layout_below="@+id/hslc_ImageView"
                android:layout_marginTop="33dp"
                android:textColor="#000000"
                android:text="your hs" />

            <Button
                android:id="@+id/Browse_hs_image"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/Browse_sign_image"
                android:layout_below="@+id/hslc_ImageView"
                android:layout_marginTop="21dp"
                android:text="Browse HS" />

            <ImageView
                android:id="@+id/hs_ImageView"
                android:layout_width="170px"
                android:layout_height="100px"
                android:layout_alignStart="@+id/Browse_sign_image"
                android:layout_below="@+id/Browse_hs_image"
                android:layout_marginStart="28dp"
                android:layout_marginTop="16dp"
                />
            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView11"
                android:layout_below="@+id/hs_ImageView"
                android:layout_marginTop="33dp"
                android:textColor="#000000"
                android:text="your graduation" />

            <Button
                android:id="@+id/Browse_graduation_image"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/Browse_sign_image"
                android:layout_below="@+id/hs_ImageView"
                android:layout_marginTop="21dp"
                android:text="Browse GRADUATION" />

            <ImageView
                android:id="@+id/graduation_ImageView"
                android:layout_width="170px"
                android:layout_height="100px"
                android:layout_alignStart="@+id/Browse_sign_image"
                android:layout_below="@+id/Browse_graduation_image"
                android:layout_marginStart="28dp"
                android:layout_marginTop="16dp"
                />
            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView11"
                android:layout_below="@+id/graduation_ImageView"
                android:layout_marginTop="33dp"
                android:textColor="#000000"
                android:text="your posgraduation" />

            <Button
                android:id="@+id/Browse_posgraduation_image"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/Browse_sign_image"
                android:layout_below="@+id/graduation_ImageView"
                android:layout_marginTop="21dp"
                android:text="Browse POSGRADUATION" />

            <ImageView
                android:id="@+id/postgraduation_ImageView"
                android:layout_width="170px"
                android:layout_height="100px"
                android:layout_alignStart="@+id/Browse_sign_image"
                android:layout_below="@+id/Browse_posgraduation_image"
                android:layout_marginStart="28dp"
                android:layout_marginTop="16dp"
                />
            <TextView
                android:id="@+id/textView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView11"
                android:layout_below="@+id/postgraduation_ImageView"
                android:layout_marginTop="33dp"
                android:textColor="#000000"
                android:text="your cast" />

            <Button
                android:id="@+id/Browse_cast_image"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/Browse_sign_image"
                android:layout_below="@+id/postgraduation_ImageView"
                android:layout_marginTop="21dp"
                android:text="Browse CAST" />

            <ImageView
                android:id="@+id/cast_ImageView"
                android:layout_width="170px"
                android:layout_height="100px"
                android:layout_alignStart="@+id/Browse_sign_image"
                android:layout_below="@+id/Browse_cast_image"
                android:layout_marginStart="28dp"
                android:layout_marginTop="16dp"
                />
            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView11"
                android:layout_below="@+id/cast_ImageView"
                android:layout_marginTop="33dp"
                android:textColor="#000000"
                android:text="your prc" />

            <Button
                android:id="@+id/Browse_prc_image"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/Browse_sign_image"
                android:layout_below="@+id/cast_ImageView"
                android:layout_marginTop="21dp"
                android:text="Browse PRC" />

            <ImageView
                android:id="@+id/prc_ImageView"
                android:layout_width="170px"
                android:layout_height="100px"
                android:layout_alignStart="@+id/Browse_sign_image"
                android:layout_below="@+id/Browse_prc_image"
                android:layout_marginStart="28dp"
                android:layout_marginTop="16dp"
                />

            <Button
                android:id="@+id/Uploads_document_image"
                android:layout_width="310px"
                android:layout_height="wrap_content"
                android:text="Upload Document"
                android:layout_below="@+id/prc_ImageView"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="140dp"
                 />




        </RelativeLayout>
    </ScrollView>
</RelativeLayout>
